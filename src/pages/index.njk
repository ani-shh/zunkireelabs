        <defs>
          <linearGradient id="inflowGrad" x1="0%" y1="0%" x2="100%" y2="0%">
            <stop offset="0%" stop-color="#d1d5db" />
            <stop offset="100%" stop-color="#eb1600" />
          </linearGradient>
          <linearGradient id="outflowGrad" x1="0%" y1="0%" x2="100%" y2="0%">
            <stop offset="0%" stop-color="#eb1600" />
            <stop offset="100%" stop-color="#d1d5db" />
          </linearGradient>
          <filter id="glow" x="-50%" y="-50%" width="200%" height="200%">
            <feGaussianBlur stdDeviation="2" result="coloredBlur"/>
            <feMerge>
              <feMergeNode in="coloredBlur"/>
              <feMergeNode in="SourceGraphic"/>
            </feMerge>
          </filter>
        </defs>
        <g id="connector-paths"></g>
        <g id="connector-dots"></g>
      </svg>

      {# JavaScript for dynamic connector positioning #}
      <script>
        (function() {
          function drawConnectors() {
            const container = document.getElementById('architecture-diagram');
            const svg = document.getElementById('connector-svg');
            const pathsGroup = document.getElementById('connector-paths');
            const dotsGroup = document.getElementById('connector-dots');

            if (!container || !svg || !pathsGroup || !dotsGroup) return;

            // Get container bounds for relative positioning
            const containerRect = container.getBoundingClientRect();

            // Get center element
            const center = document.querySelector('[data-connector="center"]');
            if (!center) return;
            const centerRect = center.getBoundingClientRect();

            // Calculate center point relative to container
            const centerLeft = centerRect.left - containerRect.left;
            const centerRight = centerRect.right - containerRect.left;
            const centerY = centerRect.top - containerRect.top + centerRect.height / 2;

            // Clear existing paths and dots
            pathsGroup.innerHTML = '';
            dotsGroup.innerHTML = '';

            // Get all source and target elements
            const sources = [1, 2, 3, 4].map(i => document.querySelector(`[data-connector="source-${i}"]`));
            const targets = [1, 2, 3, 4].map(i => document.querySelector(`[data-connector="target-${i}"]`));

            // Draw inflow paths (sources → center)
            sources.forEach((source, index) => {
              if (!source) return;
              const rect = source.getBoundingClientRect();
              const startX = rect.right - containerRect.left;
              const startY = rect.top - containerRect.top + rect.height / 2;
              const endX = centerLeft;
              const endY = centerY;

              // Control points for smooth Bézier curve
              const cp1x = startX + (endX - startX) * 0.4;
              const cp1y = startY;
              const cp2x = startX + (endX - startX) * 0.6;
              const cp2y = endY;

              const pathId = `flowIn${index + 1}`;
              const path = document.createElementNS('http://www.w3.org/2000/svg', 'path');
              path.setAttribute('id', pathId);
              path.setAttribute('d', `M ${startX} ${startY} C ${cp1x} ${cp1y}, ${cp2x} ${cp2y}, ${endX} ${endY}`);
              path.setAttribute('stroke', 'url(#inflowGrad)');
              path.setAttribute('stroke-width', '2');
              path.setAttribute('fill', 'none');
              path.setAttribute('opacity', '0.6');
              path.setAttribute('stroke-dasharray', '6 4');
              pathsGroup.appendChild(path);

              // Animated dot
              const circle = document.createElementNS('http://www.w3.org/2000/svg', 'circle');
              circle.setAttribute('r', '4');
              circle.setAttribute('fill', '#eb1600');
              circle.setAttribute('filter', 'url(#glow)');

              const animateMotion = document.createElementNS('http://www.w3.org/2000/svg', 'animateMotion');
              animateMotion.setAttribute('dur', `${1.8 + index * 0.1}s`);
              animateMotion.setAttribute('repeatCount', 'indefinite');
              animateMotion.setAttribute('begin', `${index * 0.3}s`);

              const mpath = document.createElementNS('http://www.w3.org/2000/svg', 'mpath');
              mpath.setAttribute('href', `#${pathId}`);
              animateMotion.appendChild(mpath);

              const animateOpacity = document.createElementNS('http://www.w3.org/2000/svg', 'animate');
              animateOpacity.setAttribute('attributeName', 'opacity');
              animateOpacity.setAttribute('values', '0;1;1;0');
              animateOpacity.setAttribute('keyTimes', '0;0.1;0.9;1');
              animateOpacity.setAttribute('dur', `${1.8 + index * 0.1}s`);
              animateOpacity.setAttribute('repeatCount', 'indefinite');
              animateOpacity.setAttribute('begin', `${index * 0.3}s`);

              circle.appendChild(animateMotion);
              circle.appendChild(animateOpacity);
              dotsGroup.appendChild(circle);
            });

            // Draw outflow paths (center → targets)
            targets.forEach((target, index) => {
              if (!target) return;
              const rect = target.getBoundingClientRect();
              const startX = centerRight;
              const startY = centerY;
              const endX = rect.left - containerRect.left;
              const endY = rect.top - containerRect.top + rect.height / 2;

              // Control points for smooth Bézier curve
              const cp1x = startX + (endX - startX) * 0.4;
              const cp1y = startY;
              const cp2x = startX + (endX - startX) * 0.6;
              const cp2y = endY;

              const pathId = `flowOut${index + 1}`;
              const path = document.createElementNS('http://www.w3.org/2000/svg', 'path');
              path.setAttribute('id', pathId);
              path.setAttribute('d', `M ${startX} ${startY} C ${cp1x} ${cp1y}, ${cp2x} ${cp2y}, ${endX} ${endY}`);
              path.setAttribute('stroke', 'url(#outflowGrad)');
              path.setAttribute('stroke-width', '2');
              path.setAttribute('fill', 'none');
              path.setAttribute('opacity', '0.6');
              path.setAttribute('stroke-dasharray', '6 4');
              pathsGroup.appendChild(path);

              // Animated dot
              const circle = document.createElementNS('http://www.w3.org/2000/svg', 'circle');
              circle.setAttribute('r', '4');
              circle.setAttribute('fill', '#eb1600');
              circle.setAttribute('filter', 'url(#glow)');

              const animateMotion = document.createElementNS('http://www.w3.org/2000/svg', 'animateMotion');
              animateMotion.setAttribute('dur', `${1.8 + index * 0.1}s`);
              animateMotion.setAttribute('repeatCount', 'indefinite');
              animateMotion.setAttribute('begin', `${1.2 + index * 0.3}s`);

              const mpath = document.createElementNS('http://www.w3.org/2000/svg', 'mpath');
              mpath.setAttribute('href', `#${pathId}`);
              animateMotion.appendChild(mpath);

              const animateOpacity = document.createElementNS('http://www.w3.org/2000/svg', 'animate');
              animateOpacity.setAttribute('attributeName', 'opacity');
              animateOpacity.setAttribute('values', '0;1;1;0');
              animateOpacity.setAttribute('keyTimes', '0;0.1;0.9;1');
              animateOpacity.setAttribute('dur', `${1.8 + index * 0.1}s`);
              animateOpacity.setAttribute('repeatCount', 'indefinite');
              animateOpacity.setAttribute('begin', `${1.2 + index * 0.3}s`);

              circle.appendChild(animateMotion);
              circle.appendChild(animateOpacity);
              dotsGroup.appendChild(circle);
            });
          }

          // Debounce function for resize
          function debounce(func, wait) {
            let timeout;
            return function executedFunction(...args) {
              const later = () => {
                clearTimeout(timeout);
                func(...args);
              };
              clearTimeout(timeout);
              timeout = setTimeout(later, wait);
            };
          }

          // Initialize on DOM ready and window load
          if (document.readyState === 'loading') {
            document.addEventListener('DOMContentLoaded', drawConnectors);
          } else {
            drawConnectors();
          }

          // Redraw after images/fonts load
          window.addEventListener('load', drawConnectors);

          // Redraw on resize (debounced)
          window.addEventListener('resize', debounce(drawConnectors, 100));
        })();
      </script>
    </div>
  </div>
</section>

